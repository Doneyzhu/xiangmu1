function doDelAll(){

	var ids ="";

  var oo=document.getElementsByName("check") 
  for(i=0;i<oo.length;i++) 
  if(oo[i].checked){
   
   ids = ids+","+oo[i].value;
  
  }
  
  if(ids.length>0){
   del();
   }else{
   alert("请选择要删除的岗位");
   }
}



function del(){
	
	if(confirm("您确定要删除吗？")){
